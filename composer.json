{
  "name": "dreamonkey/laravel-onesignal-rest-api",
  "description": "OneSignal REST API Wrapper For Laravel/Lumen",
  "keywords": ["onesignal", "webpush", "push", "laravel", "laravel 5", "lumen", "rest"],
  "type": "library",
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.2",
    "illuminate/support": "4.*|5.*|6.*",
    "symfony/psr-http-message-bridge": "^1.0.0"
  },
  "require-dev": {
    "vlucas/phpdotenv": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Dreamonkey\\OneSignal\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dreamonkey\\OneSignal\\OneSignalServiceProvider"
      ],
      "aliases": {
        "OneSignal": "Dreamonkey\\OneSignal\\Facades\\OneSignal"
      }
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Paolo Caleffi",
      "email": "p.caleffi@dreamonkey.com",
      "homepage": "https://dreamonkey.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
